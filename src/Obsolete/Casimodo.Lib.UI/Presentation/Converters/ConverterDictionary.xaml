<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converter="clr-namespace:Casimodo.Lib.Presentation.Converters">

    <converter:BooleanNotConverter
        x:Key="BooleanNotConverter" />

    <converter:BooleanToOpacityConverter
        x:Key="BooleanToOpacityConverter" />

    <converter:BooleanToVisibilityConverter
        x:Key="BooleanToVisibilityConverter" />

    <converter:DateTimeToStringConverter
        x:Key="DateTimeToStringConverter" />

    <converter:DateToStringConverter
        x:Key="DateToStringConverter" />

    <converter:HourMinuteToStringConverter
        x:Key="HourMinuteToStringConverter" />

    <converter:IsNullConverter
        x:Key="IsNullConverter" />    

</ResourceDictionary>
